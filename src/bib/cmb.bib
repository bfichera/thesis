@article{cremin_photoenhanced_2019,
	title = {Photoenhanced metastable c-axis electrodynamics in stripe-ordered cuprate {La} $_{\textrm{1.885}}$ {Ba} $_{\textrm{0.115}}$ {CuO} $_{\textrm{4}}$},
	volume = {116},
	issn = {0027-8424, 1091-6490},
