@article{fiebig_ultrafast_2008,
	title = {Ultrafast magnetization dynamics of antiferromagnetic compounds},
	volume = {41},
	issn = {0022-3727, 1361-6463},
	url = {https://iopscience.iop.org/article/10.1088/0022-3727/41/16/164005},
	doi = {10.1088/0022-3727/41/16/164005},
	abstract = {In three different antiferromagnetic (AFM) materials we show that the magnetization dynamics of AFM compounds differs noticeably from that of ferromagnetic compounds. Optical second harmonic generation and linear reﬂection were used to monitor with a temporal resolution of {\textless}1 ps the evolution of the AFM order parameter subsequent to an intense optical excitation. Comparison of the dynamical properties of the model antiferromagnet Cr2O3 with model ferro- and ferrimagnets reveals that spin–lattice relaxation as the temporally limiting thermalization process of the magnetic subsystem is more complex and inherently faster than in compounds displaying a spontaneous magnetization. The exchange-bias compound NiO exhibits an ultrafast photoinduced AFM phase transition in the course of which the AFM order parameter is switched by 90◦. Properly timed sequences of pump pulses can repeatedly reorient the AFM order parameter within 10 ps, thus demonstrating ultrafast AFM switching. In the colossal-magnetoresistance compound Pr1−xCaxMnO3 optical pumping triggers a transition from an insulating AFM to a conducting metallic state within 230 fs.},
	language = {en},
	number = {16},
	urldate = {2021-12-24},
	journal = {J. Phys. D: Appl. Phys.},
	author = {Fiebig, Manfred and Duong, Nguyen Phuc and Satoh, Takuya and Van Aken, Bas B and Miyano, Kenjiro and Tomioka, Yasuhide and Tokura, Yoshinori},
	month = aug,
	year = {2008},
	pages = {164005},
	file = {Fiebig et al. - 2008 - Ultrafast magnetization dynamics of antiferromagne.pdf:/home/bfichera/papers/CaMn2Bi2/Fiebig et al. - 2008 - Ultrafast magnetization dynamics of antiferromagne.pdf:application/pdf},
}

