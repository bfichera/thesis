@article{zong_ultrafast_2018,
author = {Zong, Alfred and Shen, Xiaozhe and Kogar, Anshul and Ye, Linda and Marks, Carolyn and Chowdhury, Debanjan and Rohwer, Timm and Freelon, Byron and Weathersby, Stephen and Li, Renkai and Yang, Jie and Checkelsky, Joseph and Wang, Xijie and Gedik, Nuh},
    title = {Ultrafast manipulation of mirror domain walls in a charge density wave},
    volume = {4},
    number = {10},
    elocation-id = {eaau5501},
    year = {2018},
    doi = {10.1126/sciadv.aau5501},
    publisher = {American Association for the Advancement of Science},
    abstract = {Domain walls (DWs) are singularities in an ordered medium that often host exotic phenomena such as charge ordering, insulator-metal transition, or superconductivity. The ability to locally write and erase DWs is highly desirable, as it allows one to design material functionality by patterning DWs in specific configurations. We demonstrate such capability at room temperature in a charge density wave (CDW), a macroscopic condensate of electrons and phonons, in ultrathin 1T-TaS2. A single femtosecond light pulse is shown to locally inject or remove mirror DWs in the CDW condensate, with probabilities tunable by pulse energy and temperature. Using time-resolved electron diffraction, we are able to simultaneously track anti-synchronized CDW amplitude oscillations from both the lattice and the condensate, where photoinjected DWs lead to a red-shifted frequency. Our demonstration of reversible DW manipulation may pave new ways for engineering correlated material systems with light.},
    URL = {http://advances.sciencemag.org/content/4/10/eaau5501},
    journal = {Science Advances}
}

