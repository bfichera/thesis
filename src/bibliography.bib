	journal = {Proc. Natl. Acad. Sci. U.S.A.},
	author = {Cremin, Kevin A. and Zhang, Jingdi and Homes, Christopher C. and Gu, G. D. and Sun, Zhiyuan and Fogler, Michael M. and Millis, Andrew J. and Basov, D. N. and Averitt, Richard D.},
